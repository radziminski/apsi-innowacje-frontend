{
  "name": "innowacje-frontend",
  "version": "0.1.0",
  "engines": {
    "npm": "^7 | ^8",
    "node": "^14.16.0"
  },
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "^2.8.3",
    "@reduxjs/toolkit": "^1.6.2",
    "axios": "^0.24.0",
    "axios-logger": "^2.5.0",
    "axios-retry": "^3.2.4",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "path": "^0.12.7",
    "pretty-bytes": "^5.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-hook-form": "^7.18.0",
    "react-icons": "^4.3.1",
    "react-query": "^3.31.0",
    "react-router-dom": "^5.3.0",
    "react-redux": "^7.2.6",
    "react-select": "^5.2.0",
    "react-svg": "^14.0.17",
    "styled-components": "^5.3.3",
    "styled-normalize": "^8.0.7",
    "styled-system": "^5.1.5",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "cross-env ENV_FILE=dev NODE_ENV=development webpack serve --config ./webpack/webpack.dev.js",
    "start:prod": "cross-env ENV_FILE=prod NODE_ENV=production webpack serve --config ./webpack/webpack.prod.js",
    "clean": "rimraf ./build",
    "clean:clients": "rimraf src/api-client/dotnet && rimraf src/api-client/java",
    "build": "npm run clean && cross-env ENV_FILE=prod NODE_ENV=production && webpack build --config ./webpack/webpack.prod.js",
    "test": "jest",
    "lint": "eslint ./src/ --ext js,jsx,ts,tsx",
    "lint:fix": "eslint ./src/ --ext js,jsx,ts,tsx --fix",
    "prepare": "husky install",
    "api:dotnet:generate": "openapi-generator-cli generate -i https://apsi-backend-dotnet.azurewebsites.net/swagger/v1/swagger.json -g typescript-axios -o src/api-client/dotnet --additional-properties=typescriptThreePlus=true,useSingleRequestParameter=true,supportsES6=true",
    "api:java:generate": "openapi-generator-cli generate -i https://apsi-backend-java.azurewebsites.net/v2/api-docs -g typescript-axios -o src/api-client/java --additional-properties=typescriptThreePlus=true,useSingleRequestParameter=true,supportsES6=true",
    "api:generate": "npm run clean:clients && npm run api:dotnet:generate && npm run api:java:generate"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@openapitools/openapi-generator-cli": "^2.4.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-router-dom": "^5.3.2",
    "@types/styled-components": "^5.1.15",
    "@types/styled-system": "^5.1.13",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.5.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.0.0",
    "eslint-watch": "^7.0.0",
    "eslint-webpack-plugin": "^3.1.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "^27.3.1",
    "jest-watch-typeahead": "^1.0.0",
    "prettier": "^2.4.1",
    "react-refresh": "^0.10.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "printWidth": 120
  }
}
